# Supabase Edge Functions Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_CLOUD_PROJECT_NUMBER=your_project_number
GCP_LOCATION=us
GCP_OCR_PROCESSOR_ID=your_ocr_processor_id
GCP_FORM_PARSER_PROCESSOR_ID=your_form_parser_processor_id
GCP_LAYOUT_PARSER_PROCESSOR_ID=your_layout_parser_processor_id
GCP_SUMMARIZER_PROCESSOR_ID=your_summarizer_processor_id

# Google Service Account Key (JSON string)
GOOGLE_SERVICE_ACCOUNT_KEY='{"type":"service_account","project_id":"..."}'

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Rate Limiting Configuration (optional)
RATE_LIMIT_MAX_TOKENS=600
RATE_LIMIT_REFILL_RATE=600

# Processing Configuration (optional)
MAX_CONCURRENT_REQUESTS=10
MAX_BATCH_SIZE=50
CHUNK_SIZE=1500
CHUNK_OVERLAP=200